Funkcja pivota. }}\par
{
\pard\plain \s41\li360\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid \{\par
        int* poz_pivota = Obroc_Pivot();\par
        int temp;\par
\par
        temp = k_zmienne[poz_pivota[1]];\par
        k_zmienne[poz_pivota[1]] = w_zmienne[poz_pivota[0]];\par
        w_zmienne[poz_pivota[0]] = temp;\par
\par
        powt_test[0] = powt_test1[0];\par
        powt_test1[1] = powt_test1[1];\par
        powt_test1[0] = poz_pivota[0];\par
        powt_test1[1] = poz_pivota[1];\par
\par
        double wartosc = macierz[poz_pivota[0]][poz_pivota[1]];\par
\par
        for(int x = 0; x < kolumny; x++)\par
        \{\par
                macierz[poz_pivota[0]][x] /= wartosc;\par
        \}\par
\par
        for(int u1 = 0; u1 < wiersze; u1++)\par
        \{\par
