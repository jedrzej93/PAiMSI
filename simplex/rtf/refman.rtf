\par
{\bkmkstart AAAAAAAAAF}
{\bkmkend AAAAAAAAAF}
{
\pard\plain \s51\li360\sa60\sb30\qj\widctlpar\qj\adjustright \fs20\cgrid 
\par
{
{\s17\sa60\sb30\widctlpar\qj \fs22\cgrid 
Funkcja obracajaca pivota. }}\par
{
\pard\plain \s41\li360\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid \{\par
        bool* cel_test = new bool[kolumny];\par
        int *poz_pivota = new int[2];\par
\par
        poz_pivota[0] = 0;\par
        poz_pivota[1] = 0;\par
\par
        double wsp = -1; // wspolczynnik\par
\par
        for(int i = 0; i < kolumny - 1; i++)\par
        \{\par
                if(macierz[wiersze - 1][i] > 0)\par
                        cel_test[i] = 1;\par
        \}\par
\par
        for(int j = 0; j < kolumny - 1; j++)\par
        \{\par
                if(cel_test[j] == 1)\par
                \{\par
                        for(int k = 0; k < wiersze - 1; k++)\par
                        \{\par
                                if(macierz[k][j] != 0 && macierz[k][j] > 0 && abs(macierz[k][kolumny - 1]) / macierz[k][j] <= wsp || wsp == -1)\par
                                \{\par
                                        poz_pivota[0] = k;\par
                                        poz_pivota[1] = j;\par
\par
                                        wsp = abs(macierz[poz_pivota[0]][kolumny - 1] / macierz[poz_pivota[0]][poz_pivota[1]]);\par
                                \}\par
                        \}\par
                \}\par
        \}\par
        cout << "Punkt obrotu znajduje sie (" << poz_pivota[0] + 1 << "," << poz_pivota[1] + 1 << ") o wartosci: " << macierz[poz_pivota[0]][poz_pivota[1]] << endl; \par
\par
        return poz_pivota;\par
\}\par
}
}
